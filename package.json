{
  "name": "timelines",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "fixLint": "eslint src --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
